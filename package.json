{
  "name": "contractify",
  "version": "0.0.1",
  "description": "Create and implement REST API contracts.",
  "author": "Benjamin Kaul",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "commit": "cz",
    "release": "standard-version --no-verify",
    "prepare": "husky install",
    "build": "tsc",
    "serve": "tsc --watch",
    "format": "prettier --write --ignore-unknown"
  },
  "keywords": [
    "REST",
    "API",
    "Contract"
  ],
  "dependencies": {
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "@tsconfig/node14": "1.0.1",
    "@types/node": "16.3.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.2.0",
    "husky": "7.0.1",
    "lint-staged": "11.0.1",
    "prettier": "2.3.2",
    "standard-version": "9.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
